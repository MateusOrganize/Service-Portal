<payload>&lt;?xml version="1.0" encoding="UTF-8"?&gt;&lt;record_update table="sp_widget"&gt;&lt;sp_widget action="INSERT_OR_UPDATE"&gt;&lt;category&gt;custom&lt;/category&gt;&lt;client_script&gt;&lt;![CDATA[function() {
	/* widget controller */
	var c = this;
	c.loading = true;
	c.error = false;
	
	c.title = "";

	c.loadData = function() {
		c.loading = true;
		c.server.get({
			"action": "getData",
			"table": c.options.table,
			"id": c.options.record,
			"fields": c.options.fields,
			"title": c.options.title
		}).then(function(response) {
			if(!response.data.error) {
				c.fields = response.data.obj;

				if(response.data.title)
					c.title = response.data.title;
				
				c.error = false;
				c.loading = false;

			} else {
				c.error = true
			}
		})
	};

	c.loadData()

}]]&gt;&lt;/client_script&gt;&lt;controller_as&gt;c&lt;/controller_as&gt;&lt;css&gt;.ocl-record-tooltip-popover {&amp;#13;
  .popover-title {&amp;#13;
    text-align: center;&amp;#13;
  }&amp;#13;
}&amp;#13;
&amp;#13;
  .ocl-record-tooltip-content {&amp;#13;
    width: 200px;&amp;#13;
    height: 90px;&amp;#13;
    overflow: hidden;&amp;#13;
  }&amp;#13;
  .ocl-record-tooltip-header {&amp;#13;
    text-align: center;&amp;#13;
    padding: 10px;&amp;#13;
    .title {&amp;#13;
      font-size: 16px;&amp;#13;
    }&amp;#13;
  }&amp;#13;
  .ocl-record-tooltip-loading {&amp;#13;
    display: flex;&amp;#13;
    align-items: center;&amp;#13;
    justify-content: center;&amp;#13;
    height: 100%;&amp;#13;
&amp;#13;
    .sp-loading-indicator {&amp;#13;
      color: black;&amp;#13;
    }&amp;#13;
  }&amp;#13;
&amp;#13;
  .ocl-record-tooltip-body {&amp;#13;
    display: flex;&amp;#13;
    flex-wrap: wrap;&amp;#13;
    overflow: hidden;&amp;#13;
  }&amp;#13;
&amp;#13;
  .ocl-record-tooltip-field {&amp;#13;
    width: 100%;&amp;#13;
    display: flex;&amp;#13;
    padding: 5px;&amp;#13;
&amp;#13;
    .value {&amp;#13;
      padding-left: 5px;&amp;#13;
    }&amp;#13;
  }&amp;#13;
&amp;#13;
  .ocl-record-tooltip-error {&amp;#13;
    display: flex;&amp;#13;
    align-items: center;&amp;#13;
    text-align: center;&amp;#13;
    height: 100%;&amp;#13;
  }&lt;/css&gt;&lt;data_table&gt;sp_instance&lt;/data_table&gt;&lt;demo_data/&gt;&lt;description/&gt;&lt;docs/&gt;&lt;field_list/&gt;&lt;has_preview&gt;true&lt;/has_preview&gt;&lt;id&gt;ocl-record-tooltip&lt;/id&gt;&lt;internal&gt;false&lt;/internal&gt;&lt;link&gt;&lt;![CDATA[function link(scope, element, attrs, controller) { 
}]]&gt;&lt;/link&gt;&lt;name&gt;OCL Record Tooltip&lt;/name&gt;&lt;option_schema&gt;[{"name":"table","section":"Data","label":"Table","type":"string"},{"name":"record","section":"Data","label":"Record","type":"string"}]&lt;/option_schema&gt;&lt;public&gt;false&lt;/public&gt;&lt;roles/&gt;&lt;script&gt;&lt;![CDATA[(function() {	
	if(!input)
		return;
	if(input.action == "getData") {
		var gr = new GlideRecord(input.table);
		if(gr.get(input.id)) {
			data.obj = {};
			data.obj = $sp.getFields(gr, input.fields);
			
			if(input.title) {
				data.title = gr.getDisplayValue(input.title);
			}
		} else {
			data.error = true;
		}
		
	}
	
})();]]&gt;&lt;/script&gt;&lt;servicenow&gt;false&lt;/servicenow&gt;&lt;sys_class_name&gt;sp_widget&lt;/sys_class_name&gt;&lt;sys_created_by&gt;adalberto&lt;/sys_created_by&gt;&lt;sys_created_on&gt;2020-03-18 16:01:34&lt;/sys_created_on&gt;&lt;sys_id&gt;b33067f21b2b4810ca618488ec4bcb46&lt;/sys_id&gt;&lt;sys_mod_count&gt;141&lt;/sys_mod_count&gt;&lt;sys_name&gt;OCL Record Tooltip&lt;/sys_name&gt;&lt;sys_package display_value="Global" source="global"&gt;global&lt;/sys_package&gt;&lt;sys_policy/&gt;&lt;sys_scope display_value="Global"&gt;global&lt;/sys_scope&gt;&lt;sys_update_name&gt;sp_widget_b33067f21b2b4810ca618488ec4bcb46&lt;/sys_update_name&gt;&lt;sys_updated_by&gt;olivia.temp&lt;/sys_updated_by&gt;&lt;sys_updated_on&gt;2021-03-31 14:26:26&lt;/sys_updated_on&gt;&lt;template&gt;&lt;![CDATA[&lt;div&gt;
  &lt;span class="{{::options.iconClass || 'glyphicon glyphicon-info-sign'}}"
        uib-popover-template="'recordTooltipTemplate'"
        popover-title="{{c.title}}"
        popover-placement="{{c.options.placement || 'auto'}}"
        popover-trigger="{{c.options.trigger || 'mouseenter'}}"
        popover-class="ocl-record-tooltip-popover {{c.options.className}}"
        popover-popup-close-delay="{{c.options.popupCloseDelay || 0}}"
        popover-popup-delay="{{c.options.popupDelay || 0}}"
        popover-is-open="{{c.options.isOpen || false}}"
        popover-animation="{{c.options.animation || true}}"
        &gt;&lt;/span&gt;
&lt;/div&gt;]]&gt;&lt;/template&gt;&lt;/sp_widget&gt;&lt;/record_update&gt;</payload>
